{
    "name": "vardump/activeusers",
    "description": "A widget showing active users",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "activeusers"
    ],
    "support": {
        "docs": "https://github.com/vardump-de/activeusers/blob/master/README.md",
        "issues": "https://github.com/vardump-de/activeusers/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "vardump.de",
            "homepage": "https://vardump.de"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "vardump\\activeusers\\": "src/"
        }
    },
    "extra": {
        "name": "Activeusers",
        "handle": "activeusers",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/vardump-de/activeusers/master/CHANGELOG.md",
        "class": "vardump\\activeusers\\Activeusers"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
